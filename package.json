{
  "name": "images-cache",
  "version": "1.0.1",
  "description": "Cache images as base64 for faster browser display",
  "license": "MIT",
  "repository": "revolunet/images-cache",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien@bouquillon.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "find ./spec -iname '*.spec.js' -exec ./node_modules/.bin/babel-node {} \\; | ./node_modules/.bin/tap-spec",
    "zuul": "./node_modules/zuul/bin/zuul -- spec/**/*.spec.js",
    "build": "./node_modules/.bin/browserify ./src/index.js -o ./dist/index.js",
    "watch": "./node_modules/.bin/watchify ./src/index.js -o ./dist/index.js"
  },
  "main": "dist/index.js",
  "keywords": [
    "node",
    "babel",
    "test",
    "tape"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "imageurl-base64-revolunet": "1.1.0",
    "q": "1.4.1"
  },
  "devDependencies": {
    "babel": "5.6.23",
    "babel-eslint": "3.1.23",
    "babelify": "6.1.2",
    "browserify": "11.0.1",
    "eslint": "1.0.0-rc-1",
    "eslint-config-airbnb": "0.0.6",
    "mockery": "1.4.0",
    "node-readme": "0.1.8",
    "tap-spec": "4.0.2",
    "tape": "4.0.0",
    "watchify": "3.3.1",
    "zuul": "3.2.0"
  }
}
